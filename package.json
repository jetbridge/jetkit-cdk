{
  "name": "jetkit",
  "description": "Cloud-native serverless anti-framework",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jetbridge/jetkit-cdk.git"
  },
  "homepage": "https://jetbridge.github.io/jetkit-cdk/",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc --build && concurrently --kill-others npm:build:*",
    "build:cdk": "npm run build -w @jetkit/cdk",
    "build:runtime": "npm run build -w @jetkit/cdk-runtime",
    "start": "tsc --build --watch"
  },
  "author": "JetBridge Software Inc.",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@ts-morph/bootstrap": "^0.9.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/convict": "^6.0.2",
    "@types/jest": "26.0.23",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "aws-lambda": "^1.0.6",
    "callsites": "^3.1.0",
    "concurrently": "^6.3.0",
    "constructs": "3.3.141",
    "husky": "^6.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "simple-markdown": "^0.7.3",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@jdpnielsen/http-error": "^1.2.0",
    "deepmerge": "^4.2.2",
    "is-plain-object": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "workspaces": [
    "packages/*"
  ]
}
