{
  "name": "@jetkit/cdk-runtime",
  "version": "2.0.0",
  "description": "Cloud-native serverless anti-framework",
  "types": "build/esm/src/index.d.ts",
  "main": "build/cjs/src/index.js",
  "exports": "./build/esm/src/index.js",
  "type": "module",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jetbridge/jetkit-cdk.git"
  },
  "homepage": "https://jetbridge.github.io/jetkit-cdk/",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "tsc --build -w -p tsconfig-cjs.json",
    "build": "npm run tsc",
    "tsc": "tsc --build tsconfig.json && tsc --build tsconfig-cjs.json",
    "docs": "typedoc",
    "prepack": "npm run build",
    "rel:patch": "npm version patch && npm publish",
    "rel:minor": "npm version minor && npm publish",
    "rel:next": "npm version patch && npm publish --tag next"
  },
  "author": "JetBridge Software Inc.",
  "license": "MIT",
  "devDependencies": {
  },
  "dependencies": {
      "@jetkit/cdk-metadata": "../metadata"
  },
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "lambda",
    "iac",
    "rest",
    "typescript",
    "api",
    "prisma",
    "postgres",
    "postgresql",
    "pg"
  ]
}
