{
  "name": "@jetkit/cdk",
  "version": "2.0.10",
  "description": "Cloud-native serverless anti-framework",
  "type": "module",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "import": "./build/esm/index.js"
    },
    "./*": {
      "require": "./build/cjs/*.js",
      "import": "./build/esm/*.js"
    }
  },
  "main": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "private": false,
  "engines": {
    "node": ">=0.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jetbridge/jetkit-cdk.git"
  },
  "homepage": "https://jetbridge.github.io/jetkit-cdk/",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "start": "tsc --build --watch",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --build tsconfig.json",
    "build:cjs": "tsc --build tsconfig-cjs.json",
    "docs": "typedoc",
    "prepack": "npm run build",
    "rel:patch": "npm version patch && npm publish",
    "rel:minor": "npm version minor && npm publish",
    "rel:next": "npm version patch && npm publish --tag next"
  },
  "author": "JetBridge Software Inc.",
  "license": "MIT",
  "devDependencies": {
    "@aws-cdk/assert": "*",
    "@prisma/client": "^3.0.1",
    "@prisma/migrate": "^3.0.1",
    "aws-cdk": "*",
    "callsites": "^3.1.0",
    "constructs": "3.3.141",
    "prisma": "^3.0.1"
  },
  "dependencies": {
    "@aws-cdk/core": "*",
    "@aws-cdk/aws-apigatewayv2": "*",
    "@aws-cdk/aws-apigatewayv2-authorizers": "*",
    "@aws-cdk/aws-apigatewayv2-integrations": "*",
    "@aws-cdk/aws-ec2": "*",
    "@aws-cdk/aws-events": "*",
    "@aws-cdk/aws-events-targets": "*",
    "@aws-cdk/aws-iam": "*",
    "@aws-cdk/aws-lambda": "*",
    "@aws-cdk/aws-lambda-nodejs": "*",
    "@aws-cdk/aws-rds": "*",
    "@aws-cdk/aws-secretsmanager": "*",
    "deepmerge": "^4.2.2",
    "is-plain-object": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.0"
  },
  "peerDependenciesMeta": {
    "prisma": {
      "optional": true
    },
    "@prisma/migrate": {
      "optional": true
    },
    "@prisma/client": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@aws-cdk/core": "<2.0.0",
    "@jetkit/cdk-runtime": "*",
    "@prisma/client": "*",
    "@prisma/migrate": "*",
    "prisma": "*"
  },
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "lambda",
    "iac",
    "rest",
    "typescript",
    "api",
    "prisma",
    "postgres",
    "postgresql",
    "pg"
  ]
}
